<h1>New Anime</h1>
<form action="/create" method="post">
  <p>
    <label>
      Name
      <input type="text" name="name">
    </label>
  </p>
  <p>
    <label>
      Creator
      <input type="text" name="creator">
    </label>
  </p>
  <p>
    <label>
      Main Character(s)
      <input type="text" name="mainChar">
    </label>
  </p>
  <p>
    <input type="submit" value="Save">
  </p>
</form>
